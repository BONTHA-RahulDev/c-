#include<iostream>
using namespace std;
int main()
{
    int a,b,temp;
    cout<<"swap two numbers"<<endl;
    cout<<"enter the value of a"<<endl;
    cin>>a;
    cout<<"enter the value of b"<<endl;
    cin>>b;
    temp=b;
    b=a;
    a=temp;
    cout<<"after swpping the a is"<< a <<endl;
    cout<<"after the swapping of b is"<< b <<endl;
    return 0 ;
}
